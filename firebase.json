{
  "projects": {
    "default": "auraa-ai-69"
  },
  "functions": [
    {
      "codebase": "default",
      "source": "functions",
      "runtime": "nodejs22",
      "ignore": [
        "**/node_modules/**",
        "**/.*"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    },
    {
      "codebase": "auraacode",
      "source": "auraacode",
      "runtime": "nodejs22",
      "ignore": [
        "**/node_modules/**",
        "**/.*"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    },
    {
      "codebase": "auraafunction",
      "source": "auraafunction",
      "runtime": "nodejs22",
      "ignore": [
        "**/node_modules/**",
        "**/.*"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/pricing",
        "destination": "/pricing.html"
      },
      {
        "source": "/checkout-return",
        "destination": "/checkout-return.html"
      },
      {
        "source": "/subscribe",
        "destination": "/subscribe.html"
      },
      {
        "source": "/cancel",
        "destination": "/cancel.html"
      },
      {
        "source": "/success",
        "destination": "/success.html"
      },
      {
        "source": "/terms",
        "destination": "/terms.html"
      },
      {
        "source": "/privacy",
        "destination": "/privacy.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      },
      {
        "source": "/assets/**",
        "headers": [
          {
            "key": "Cache-control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  },
  "emulators": {
    "dataconnect": {
      "dataDir": "dataconnect/.dataconnect/pgliteData",
      "port": 9399,
      "host": "127.0.0.1"
    },
    "auth": {
      "port": 9099,
      "host": "127.0.0.1"
    },
    "functions": {
      "port": 5001,
      "host": "127.0.0.1"
    },
    "firestore": {
      "port": 8080,
      "host": "127.0.0.1"
    },
    "hosting": {
      "port": 5000,
      "host": "127.0.0.1"
    },
    "ui": {
      "enabled": true,
      "port": 4000,
      "host": "127.0.0.1"
    },
    "database": {
      "port": 9001,
      "host": "127.0.0.1"
    },
    "pubsub": {
      "port": 8085,
      "host": "127.0.0.1"
    },
    "storage": {
      "port": 9199,
      "host": "127.0.0.1"
    },
    "eventarc": {
      "port": 9299,
      "host": "127.0.0.1"
    },
    "singleProjectMode": true
  },
  "dataconnect": {
    "source": "dataconnect"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  },
  "extensions": {},
  "database": {
    "rules": "database.rules.json"
  }
}