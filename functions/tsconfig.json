{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2022",
    "esModuleInterop": true,
    "lib": ["es2022", "dom", "esnext"],
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./node_modules"],
    "types": ["node", "firebase-functions", "@google-cloud/vertexai"]
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
